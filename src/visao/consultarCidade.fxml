<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="555.0" prefWidth="713.0" xmlns="http://javafx.com/javafx/8.0.60" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controle.ConsultarCidadeControle">
   <children>
      <TableView fx:id="tableConsultaCidade" layoutX="10.0" layoutY="14.0" prefHeight="435.0" prefWidth="699.0">
        <columns>
            <TableColumn fx:id="columnIDCidade" prefWidth="75.0" text="idCidade" visible="false" />
            <TableColumn fx:id="columnIDEstado" prefWidth="75.0" text="IdEstado" visible="false" />
          <TableColumn fx:id="columnNomeCidade" prefWidth="297.0" text="Nome da Cidade" />
          <TableColumn fx:id="columnNomeEstado" prefWidth="401.0" text="Nome do Estado Referente" />
        </columns>
      </TableView>
      <ComboBox fx:id="comboBoxTipoPesquisa" layoutX="29.0" layoutY="466.0" onAction="#comboBoxTIpoPesquisaSelecionado" prefHeight="31.0" prefWidth="305.0" promptText="Selecione o tipo de Pesquisa" />
      <TextField fx:id="textPesquisa" layoutX="354.0" layoutY="468.0" prefHeight="31.0" prefWidth="349.0" />
      <Label fx:id="lblPesquisa" layoutX="354.0" layoutY="449.0" prefHeight="21.0" prefWidth="349.0" text="Label" visible="false" />
      <Button fx:id="btnPesquisar" layoutX="353.0" layoutY="510.0" mnemonicParsing="false" onAction="#btnPesquisarClick" prefHeight="31.0" prefWidth="86.0" text="Pesquisar" />
      <Button fx:id="btnAlterar" layoutX="450.0" layoutY="510.0" mnemonicParsing="false" onAction="#btnAlterarClick" prefHeight="31.0" prefWidth="77.0" text="Alterar" />
      <Button fx:id="btnDeletar" layoutX="535.0" layoutY="510.0" mnemonicParsing="false" onAction="#btnDeletarClick" prefHeight="31.0" prefWidth="77.0" text="Deletar" />
      <ImageView fitHeight="31.0" fitWidth="59.0" layoutX="619.0" layoutY="510.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../../_imagens/bcyoyEGxi.png" />
         </image>
      </ImageView>
      <Button fx:id="btnTraduzir" layoutX="619.0" layoutY="510.0" mnemonicParsing="false" onAction="#btnTraduzirClick" opacity="0.0" prefHeight="31.0" prefWidth="59.0" />
   </children>
</AnchorPane>
