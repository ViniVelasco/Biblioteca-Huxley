         <image>
            <Image url="@../../_imagens/iconCidade.png" />
         </image>
      </ImageView>
      <TextField fx:id="textNome" layoutX="88.0" layoutY="57.0" prefHeight="31.0" prefWidth="396.0" />
      <Label fx:id="lblNome" layoutX="88.0" layoutY="36.0" prefHeight="21.0" prefWidth="138.0" text="Nome da Cidade" />
      <ComboBox fx:id="comboBoxEstado" layoutX="85.0" layoutY="100.0" prefHeight="31.0" prefWidth="402.0" promptText="Estado da Cidade" />
      <ImageView fitHeight="31.0" fitWidth="59.0" layoutX="495.0" layoutY="180.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../../_imagens/bcyoyEGxi.png" />
